{
  "name": "Bias Evaluation",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Displays the political bias of a webpage",
  "web_accessible_resources": [
    "arrow2.png",
    "dot.png"
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "css": [ "css.css" ]
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_title": "Check Bias!"
  },
  "permissions": [ "activeTab" ]
}